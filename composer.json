{
    "name": "jeffreyvanrossum/laravel-simple-media",
    "description": "A simple media package for Laravel.",
    "license": "MIT",
    "keywords": ["laravel", "media"],
    "authors": [
        {
            "name": "Jeffrey van Rossum",
            "email": "jeffrey@vanrossum.dev"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.0",
        "psalm/plugin-laravel": "^1.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Jeffreyvr\\SimpleMedia\\Tests\\": "tests"
        }
    },
    "autoload": {
        "psr-4": {
            "Jeffreyvr\\SimpleMedia\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jeffreyvr\\SimpleMedia\\SimpleMediaServiceProvider"
            ]
        }
    },
    "require": {
        "intervention/image": "^2.4@dev"
    }
}
